* {
	margin : 0;
	padding : 0;
}

.navbar-brand, .navbar-left {
	position : relative;
	left : 100px;
}

.navbar-right {
	position : relative;
	right : 100px;
}

.carousel{
	height: 85%;
	background: black;
	margin-bottom: 60px;
}

